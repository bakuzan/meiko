{
  "name": "bkz-meiko",
  "version": "6.2.1",
  "description": "Bakuzan's Component Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakuzan/meiko.git"
  },
  "main": "index.js",
  "files": [
    "*",
    "!*.tgz"
  ],
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}",
      "!lib/constants/*",
      "!lib/styles/*"
    ],
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/tools/",
      "<rootDir>/stories/",
      "<rootDir>/types/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__helpers__",
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/tools/",
      "<rootDir>/stories/",
      "<rootDir>/types/"
    ]
  },
  "lint-staged": {
    "{,lib,tools,.storybook,__tests__, types}/**/*.{,js,ts}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty && npm run test"
    }
  },
  "scripts": {
    "pretty": "lint-staged",
    "dtslint": "dtslint types",
    "test": "npx jest",
    "test:update": "npx jest -u",
    "test:watch": "npx jest -o --watch --maxWorkers=10",
    "test:coverage": "npx jest --coverage",
    "build:modules": "node ./tools/build.js",
    "storybook": "start-storybook -p 7200 -c .storybook -s public",
    "build-storybook": "build-storybook -c .storybook -o .out -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "release:test": "npm run build:modules && cd .\\build && npm pack && cd ..",
    "release:npm": "npm run build:modules && cd .\\build && npm publish && cd .."
  },
  "keywords": [
    "reactjs",
    "component library"
  ],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.0",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-docs": "^6.4.18",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.18",
    "@storybook/storybook-deployer": "^2.8.10",
    "@testing-library/react": "^9.5.0",
    "@types/react": "^16.14.23",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "ayaka": "file:../ayaka/build",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^2.4.2",
    "classnames": "^2.3.1",
    "css-loader": "^5.2.7",
    "dotenv": "^8.6.0",
    "dtslint": "^0.9.9",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "execa": "^1.0.0",
    "fetch-mock": "^8.3.2",
    "fs-extra": "^7.0.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nano-css": "^5.3.4",
    "node-sass-chokidar": "^1.5.0",
    "postcss-loader": "^4.3.0",
    "prettier": "^2.5.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^10.2.1",
    "storybook-addon-react-docgen": "^1.2.42",
    "style-loader": "^2.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.0",
    "@seznam/compose-react-refs": "^1.0.6"
  },
  "peerDependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": "^2.3.1",
    "nano-css": ">=5.3.4",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  }
}
