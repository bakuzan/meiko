{
  "name": "bkz-meiko",
  "version": "8.0.0",
  "description": "Bakuzan's Component Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakuzan/meiko.git"
  },
  "main": "index.js",
  "files": [
    "*",
    "!*.tgz"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}",
      "!lib/constants/*",
      "!lib/styles/*"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/tools/",
      "<rootDir>/stories/",
      "<rootDir>/types/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__helpers__",
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/tools/",
      "<rootDir>/stories/",
      "<rootDir>/types/"
    ]
  },
  "lint-staged": {
    "{,lib,tools,.storybook,__tests__, types}/**/*.{,js,ts}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "prepare": "husky",
    "pretty": "lint-staged",
    "dtslint": "dtslint types",
    "test": "npx jest",
    "test:update": "npx jest -u",
    "test:watch": "npx jest -o --watch --maxWorkers=10",
    "test:coverage": "npx jest --coverage",
    "build:modules": "node ./tools/build.js",
    "storybook": "start-storybook -p 7200 -c .storybook -s public",
    "build-storybook": "build-storybook -c .storybook -o .out -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "release:test": "npm run build:modules && cd .\\build && npm pack && cd ..",
    "release:npm": "npm run build:modules && cd .\\build && npm publish && cd .."
  },
  "keywords": [
    "reactjs",
    "component library"
  ],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.25.9",
    "@babel/plugin-proposal-export-default-from": "^7.25.9",
    "@babel/plugin-transform-export-namespace-from": "^7.25.9",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.25.9",
    "@babel/plugin-transform-optional-chaining": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/register": "^7.25.9",
    "@storybook/addon-a11y": "^7.6.20",
    "@storybook/addon-actions": "^7.6.20",
    "@storybook/addon-docs": "^7.6.20",
    "@storybook/addon-knobs": "^7.1.1",
    "@storybook/preset-create-react-app": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "@storybook/storybook-deployer": "^2.8.16",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.12",
    "ayaka": "file:../ayaka/build",
    "babel-core": "^7.0.0-bridge.0",
    "@babel/eslint-parser": "^7.25.9",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^4.1.2",
    "classnames": "^2.5.1",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "@definitelytyped/dtslint": "^0.2.25",
    "eslint": "^9.13.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "execa": "^4.1.0",
    "fetch-mock": "^9.11.0",
    "fs-extra": "^11.2.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nano-css": "^5.6.2",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.80.4",
    "sass-loader": "^16.0.2",
    "storybook-addon-react-docgen": "^1.2.44",
    "style-loader": "^4.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@seznam/compose-react-refs": "^1.0.6"
  },
  "peerDependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": ">=2.3.1",
    "nano-css": ">=5.3.5",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
