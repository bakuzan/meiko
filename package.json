{
  "name": "bkz-meiko",
  "version": "1.1.0",
  "description": "Bakuzan's Component Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakuzan/meiko.git"
  },
  "main": "index.js",
  "files": [
    "*",
    "!*.tgz"
  ],
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}",
      "!lib/constants/*",
      "!lib/styles/*"
    ],
    "setupFiles": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/tools/",
      "<rootDir>/stories/",
      "<rootDir>/types/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__helpers__",
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/tools/",
      "<rootDir>/stories/",
      "<rootDir>/types/"
    ]
  },
  "lint-staged": {
    "{,lib,tools,.storybook,__tests__, types}/**/*.{,js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty && npm run test"
    }
  },
  "scripts": {
    "pretty": "lint-staged",
    "dtslint": "dtslint types",
    "test": "npx jest",
    "test:update": "npx jest -u",
    "test:watch": "npx jest -o --watch --maxWorkers=10",
    "test:coverage": "npx jest --coverage",
    "build:bundle": "rollup -c",
    "build:modules": "node ./tools/build.js",
    "build:all": "npm run bundle && npm run build",
    "storybook": "start-storybook -p 7200 -c .storybook -s public",
    "build-storybook": "build-storybook -c .storybook -o .out -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "release:test": "npm run build:modules && cd .\\build && npm pack && cd ..",
    "release:npm": "npm run build:modules && cd .\\build && npm publish && cd .."
  },
  "keywords": [
    "reactjs",
    "component library"
  ],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/react": "^16.9.2",
    "ayaka": "file:../ayaka/build",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "css-loader": "^3.2.0",
    "dotenv": "^8.0.0",
    "dtslint": "^0.9.8",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "execa": "^1.0.0",
    "fetch-mock": "^6.3.0",
    "fs-extra": "^7.0.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "nano-css": "^5.2.0",
    "node-sass-chokidar": "^1.3.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "react-testing-library": "^6.0.4",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^4.2.1",
    "rollup-plugin-replace": "^2.1.1",
    "rollup-plugin-uglify": "^6.0.2",
    "sass-loader": "^8.0.0",
    "storybook-addon-react-docgen": "^1.2.15",
    "style-loader": "^1.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5"
  },
  "peerDependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": "^2.2.6",
    "nano-css": ">=5.1.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  }
}
