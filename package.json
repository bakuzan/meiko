{
  "name": "meiko",
  "version": "3.0.0-alpha",
  "private": true,
  "description": "Component Library",
  "main": "dist/index.min.js",
  "files": [
    "build",
    "dist"
  ],
  "lint-staged": {
    "{,lib,tools,.storybook}/**/*.{ts,tsx,js,jsx,json,scss}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "pre-commit": [
    "precommit-msg",
    "lint-ts",
    "lint"
  ],
  "scripts": {
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint-ts": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'",
    "lint": "lint-staged",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "rollup -c",
    "build": "node ./tools/build.js && node ./tools/postBuild.js",
    "build-all": "npm run bundle && npm run build",
    "storybook": "start-storybook -p 7200 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .out -s public",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "keywords": [
    "reactjs",
    "component-library"
  ],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.0.0",
    "@dump247/storybook-state": "^1.5.2",
    "@storybook/addon-a11y": "^5.0.0",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-info": "^5.0.0",
    "@storybook/addon-knobs": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@storybook/storybook-deployer": "^2.8.1",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "ayaka": "file:../ayaka/build",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
