{
  "name": "bkz-meiko",
  "version": "6.2.1",
  "description": "Bakuzan's Component Library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bakuzan/meiko.git"
  },
  "main": "index.js",
  "files": [
    "*",
    "!*.tgz"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "lib/**/*.{js,jsx}",
      "!lib/constants/*",
      "!lib/styles/*"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "snapshotSerializers": [],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/tools/",
      "<rootDir>/stories/",
      "<rootDir>/types/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__helpers__",
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/dist/",
      "<rootDir>/tools/",
      "<rootDir>/stories/",
      "<rootDir>/types/"
    ]
  },
  "lint-staged": {
    "{,lib,tools,.storybook,__tests__, types}/**/*.{,js,ts}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "pretty": "lint-staged",
    "dtslint": "dtslint types",
    "test": "npx jest",
    "test:update": "npx jest -u",
    "test:watch": "npx jest -o --watch --maxWorkers=10",
    "test:coverage": "npx jest --coverage",
    "build:modules": "node ./tools/build.js",
    "storybook": "start-storybook -p 7200 -c .storybook -s public",
    "build-storybook": "build-storybook -c .storybook -o .out -s public",
    "deploy-storybook": "storybook-to-ghpages",
    "release:test": "npm run build:modules && cd .\\build && npm pack && cd ..",
    "release:npm": "npm run build:modules && cd .\\build && npm publish && cd .."
  },
  "keywords": [
    "reactjs",
    "component library"
  ],
  "author": "bakuzan",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/storybook-deployer": "^2.8.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.18",
    "ayaka": "file:../ayaka/build",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.0.2",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^2.4.2",
    "classnames": "^2.3.1",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.2",
    "dtslint": "^0.9.9",
    "eslint": "^8.23.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "execa": "^1.0.0",
    "fetch-mock": "^8.3.2",
    "fs-extra": "^7.0.1",
    "husky": "^8.0.1",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2",
    "lint-staged": "^13.0.3",
    "nano-css": "^5.3.5",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "storybook-addon-react-docgen": "^1.2.42",
    "style-loader": "^2.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0",
    "@seznam/compose-react-refs": "^1.0.6"
  },
  "peerDependencies": {
    "ayaka": "file:../ayaka/build",
    "classnames": "^2.3.1",
    "nano-css": ">=5.3.5",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  }
}
